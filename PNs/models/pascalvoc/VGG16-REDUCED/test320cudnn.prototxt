  type: "Convolution"
  bottom: "fc7"
  top: "rpn_conv7"
    param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    kernel_size: 2 
    stride: 2
    engine: CUDNN
    weight_filler { 
    type: "gaussian" 
    std: 0.01 
    }
    bias_filler { 
    type: "constant" 
    value: 0 
    }
  }
}

layer {
  name: "rpn_lrn7"
  type: "BatchNorm"
  bottom: "rpn_conv7"
  top: "rpn_lrn7"
}
